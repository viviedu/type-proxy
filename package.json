{
  "name": "@viviedu/type-proxy",
  "version": "2.0.0",
  "description": "Validate data with unknown structure",
  "author": "Vivi International",
  "homepage": "https://github.com/viviedu/type-proxy",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {

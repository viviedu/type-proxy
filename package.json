    "eslint-plugin-import": "^2.29.1",
    "mocha": "^10.1.0",
    "ts-node": "^10.7.0",
    "typescript": "^5.5.3"
  },
  "keywords": [
    "parse",
    "parser",
    "parsing",
    "validate",
